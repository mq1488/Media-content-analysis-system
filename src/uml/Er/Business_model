@startuml
'-Пользователь-'
  entity User
  entity User.id
  entity User.Usersname
  entity User.Password
  entity User.Email
  entity User.Status
  entity User.Role

  object User.guest 
  object User.registred  
  object User.banned
  object User.admin
  object User.user


  
  '-Запрос-'
  entity Request
  entity Request.id
  entity Request.status

  

  object Request.ready
  object Request.in_process


  'Инфа'
  entity Processed_Data
  entity Processed_Data.id
  entity Processed_Data.type




  entity Untreated_Data
  entity Untreated_Data.text
  entity Untreated_Data.graph
  entity Untreated_Data.diagrams 
  entity Untreated_Data.statistics

 
  'Инструмент'
  entity Tools
  entity Tools.name
  entity Tools.id
  
   
   
  User.id -u-* User
  User.Usersname -u-* User
  User.Password -u-* User
  User.Status -u-* User
  User.Email -u-* User
  User.Role -u-* User


  User.guest .u.> User.Status : instanceOf
  User.registred .u.> User.Status : instanceOf
  User.banned .u.> User.Status : instanceOf
  User.admin .u.> User.Role : instanceOf
  User.user .u.> User.Role : instanceOf


  Request.id -u-* Request
  Request.status -u-* Request

  Tools.id -u-* Tools
  Tools.name -u-* Tools

  Processed_Data.id -u-* Processed_Data
  Processed_Data.type -u-* Processed_Data

  Untreated_Data.text-u-* Untreated_Data
  Untreated_Data.graph -u-* Untreated_Data
  Untreated_Data.diagrams -u-* Untreated_Data
  Untreated_Data.statistics -u-* Untreated_Data
 


  Request.ready .u.> Request.status : instanceOf
  Request.in_process .u.> Request.status : instanceOf



  User "(0,*)"-u-"(1,1)" Request
  Request "(0,*)"-u-"(1,1)" Processed_Data 
  Processed_Data "(0,*)"-u-"(1,1)" Tools
  Tools "(0,*)"-u-"(1,1)" Untreated_Data
@enduml
